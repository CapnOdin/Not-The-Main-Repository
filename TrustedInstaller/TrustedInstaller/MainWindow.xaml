<Window x:Class="TrustedInstaller.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrustedInstaller"
		xmlns:Model="clr-namespace:TrustedInstaller.Model"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*"/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>

		<Canvas Grid.Row="0" />

		<Button Grid.Row="1" Content="Click" Click="OnButtonClick" Command="{Binding Model:model.customCommandName}" />

	</Grid>
</Window>
